<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="s6.suiviRegime.modele.AlimentationConseil" table="alimentationconseil">
        <meta attribute="class-description">
            This class contains the matiere detail.
        </meta>
        <id name="id" type="int" column="idalimentationconseil">
            <generator class="sequence">
            	<param name="sequence_name">alimentationconseil_idalimentationconseil_seq</param>
            </generator>
        </id>
        <property name="nom" column="nomalimentationconseil" type="string"/>
        <property name="matin" column="matinalimentationconseil" type="string"/>
        <property name="midi" column="midialimentationconseil" type="string"/>
        <property name="soir" column="soiralimentationconseil" type="string"/>
    </class>
</hibernate-mapping>